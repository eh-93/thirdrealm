{
  "name": "thirdrealm",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./styles/tailwind.css -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "chaindexer": "ts-node chaindexer/main.ts"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@alch/alchemy-sdk": "^1.2.0",
    "@alch/alchemy-web3": "^1.4.6",
    "@editorjs/code": "^2.7.0",
    "@editorjs/editorjs": "^2.25.0",
    "@editorjs/embed": "^2.5.2",
    "@editorjs/header": "^2.6.2",
    "@editorjs/image": "^2.6.2",
    "@editorjs/link": "^2.4.1",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@editorjs/simple-image": "^1.4.1",
    "@editorjs/table": "^2.0.2",
    "@editorjs/warning": "^1.2.0",
    "@headlessui/react": "^1.6.4",
    "@isaacs/ttlcache": "^1.0.5",
    "@prisma/client": "^4.1.0",
    "@rainbow-me/rainbowkit": "^0.4.4",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "@remix-run/server-runtime": "*",
    "bcryptjs": "^2.4.3",
    "isbot": "^3.5.0",
    "@udecode/plate": "^14.4.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "editorjs-blocks-react-renderer": "^1.2.4",
    "editorjs-react-renderer": "^3.2.0",
    "ethers": "^5.5.1",
    "express": "^4.18.1",
    "is-hotkey": "^0.2.0",
    "lru-cache": "^7.10.2",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0",
    "react-editor-js": "^2.1.0",
    "react-jazzicon": "^1.0.3",
    "react-uploader": "^1.8.0",
    "remix-utils": "^3.3.0",
    "siwe": "^1.1.6",
    "slate": "^0.81.1",
    "slate-history": "^0.66.0",
    "slate-hyperscript": "^0.77.0",
    "slate-react": "^0.81.0",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.5",
    "wagmi": "^0.5.11",
    "wallet-address-validator": "^0.2.4",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/editorjs__header": "^2.6.0",
    "@types/eslint": "^8.4.5",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "cypress": "^9.6.1",
    "esbuild": "^0.14.39",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.3",
    "msw": "^0.44.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^4.0.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.6",
    "@tailwindcss/forms": "^0.5.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.18.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
